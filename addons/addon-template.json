      "dialog-template.html": {
        "_id": "dialog-template.html",
        "source": "<div class=\"modal fade\" id=\"designer-dialog-template\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"templateLabel\">\n    <div class=\"modal-dialog modal-lg\" role=\"document\">\n        <div class=\"modal-content\">\n            <div class=\"modal-header\">\n                <h4 class=\"modal-title\" id=\"templateLabel\">{{title}}</h4>\n            </div>\n            <div class=\"modal-body\">\n                {{message}}\n            </div>\n            <div class=\"modal-footer\">\n                <button type=\"button\" class=\"btn btn-default\" id=\"designer-dialog-template-cancel\">Cancel</button>\n                <button type=\"button\" class=\"btn btn-primary\" id=\"designer-dialog-template-ok\">OK</button>\n            </div>\n        </div>\n    </div>\n</div>"
      },

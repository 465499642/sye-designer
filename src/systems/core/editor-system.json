    "p1e0b1155ea1f248": {
      "_id": "p1e0b1155ea1f248",
      "component": "1f1781882618115",
      "state": "click",
      "action": "function click() { \n  var editor = this.require('editor'),\n    designer = this.require('designer'),\n    extra = {},\n    data = {};\n\n  if (editor.getValue().indexOf('{') === 0) {\n    this.require('store').data(JSON.parse(editor.getValue()));\n  } else {\n    extra = this.require('store').extra();\n    if (extra && extra.context) {\n      data = this.require('store').data();\n      data[extra.context] = editor.getValue();\n      this.require('store').data(data);\n    }\n  }\n\n  extra = {};\n  extra.context = 'name';\n  this.require('store').extra(extra);\n\n  editor.setEditor('text', this.require('store').data().name, 1);\n}",
      "useCoreAPI": false,
      "core": false
    },
